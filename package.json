{
  "name": "@jsonstack/data",
  "version": "1.1.3",
  "description": "jsonstack data is a TypeScript module with simple and efficient tools for data mining and data analysis in TypeScript. When jsonstack data used with ML.js, pandas-js, and numjs, you're left with the equivalent R/Python tool set in TypeScript.",
  "main": "dist/index.cjs.js",
  "cjs": "dist/index.cjs.js",
  "module": "build/index.js",
  "esm": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "web": "dist/index.web.js",
  "modules.root": "build",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/repetere/jsonm-data.git"
  },
  "author": {
    "name": "Yaw Joseph Etse",
    "email": "yaw.etse@gmail.com"
  },
  "keywords": [
    "Machine Learning",
    "data science",
    "scaling",
    "feature scaling",
    "feature engineering",
    "scalars",
    "scikit-learn",
    "sk-learn",
    "ml",
    "pre-processing",
    "data preparation",
    "pandas",
    "numpy"
  ],
  "bugs": {
    "url": "https://github.com/repetere/jsonm-data/issues"
  },
  "homepage": "https://github.com/repetere/jsonm-data#readme",
  "scripts": {
    "dev": "rollup -c -w",
    "benchmark": "node -r esm  benchmark/main_bmrk.js",
    "test": "jest --coverage",
    "doc": "tsc && typedoc && sitedown manual/ -b docs/manual",
    "compile": "tsc",
    "build": "tsc && rollup -c && typedoc && sitedown manual/ -b docs/manual",
    "coverage": "coveralls"
  },
  "dependencies": {
    "csvtojson": "^2.0.10",
    "js-grid-search-lite": "^1.3.3",
    "lodash.range": "^3.2.0",
    "lodash.rangeright": "^4.2.0",
    "ml-confusion-matrix": "^0.4.0",
    "ml-matrix": "^6.4.1",
    "ml-stat": "^1.3.3",
    "node-fpgrowth": "^1.2.1",
    "probability-distributions": "^0.9.1",
    "random-js": "^2.1.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.2",
    "@types/jest": "^26.0.22",
    "@types/lodash.range": "3.2.6",
    "@types/lodash.rangeright": "4.2.6",
    "@types/natural": "^2.1.1",
    "@types/probability-distributions": "0.1.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/stemmer": "2.0.0",
    "@types/valid-url": "1.0.3",
    "husky": "^6.0.0",
    "install": "0.13.0",
    "jest": "^26.6.3",
    "ml-cart": "^2.0.2",
    "ml-knn": "3.0.0",
    "ml-random-forest": "^2.0.2",
    "ml-regression-multivariate-linear": "2.0.3",
    "puppeteer": "^9.0.0",
    "rollup": "^2.44.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-terser-js": "1.0.2",
    "stemmer": "1.0.5",
    "ts-jest": "^26.5.4",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.34",
    "typescript": "^4.2.3",
    "underscore": "^1.12.1"
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "Release: ${nextRelease.version}\n\n${nextRelease.notes}"
      }
    ],
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/changelog"
    ]
  }
}
